# Environment Variables Example
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# MongoDB
MONGODB_CONNECTION_STRING=mongodb://admin:YOUR_PASSWORD@localhost:27017

# Redis
REDIS_CONNECTION_STRING=localhost:6379,password=YOUR_PASSWORD,abortConnect=false

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_VHOST=sanseal
RABBITMQ_USER=admin
RABBITMQ_PASS=YOUR_PASSWORD

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=YOUR_PASSWORD
MINIO_PUBLIC_ENDPOINT=https://storage.nhatcuong.io.vn

# JWT
JWT_SECRET=YOUR_SUPER_SECRET_KEY_AT_LEAST_32_CHARACTERS_LONG

# Google OAuth2
GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=YOUR_GOOGLE_CLIENT_SECRET

# Firebase FCM
FIREBASE_PROJECT_ID=your-firebase-project-id

# Shopee Affiliate
SHOPEE_APP_ID=YOUR_SHOPEE_APP_ID
SHOPEE_SECRET=YOUR_SHOPEE_SECRET

# Lazada Affiliate
LAZADA_APP_KEY=YOUR_LAZADA_APP_KEY
LAZADA_APP_SECRET=YOUR_LAZADA_APP_SECRET

# TikTok Affiliate
TIKTOK_APP_ID=YOUR_TIKTOK_APP_ID
TIKTOK_APP_SECRET=YOUR_TIKTOK_APP_SECRET
